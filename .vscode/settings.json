{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "css.validate": false,
  "scss.validate": false,
  "editor.formatOnSave": false,
  "search.exclude": {
    "**/node_modules/": true,
    ".next/": true
  }
}
